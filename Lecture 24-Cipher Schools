#include <iostream>
using namespace std;
void swap(int *x, int *y){
    int c;
    c = *x;
    *x=*y;
    *y=c;
    cout<<"Swapping is complete";
}
int main() {
    int a=3;
    int b=4;
    cout<<"before swap calues are "<<a<<" "<<b<<endl;
    swap(&a,&b);
    cout<<"After swap calues are "<<a<<" "<<b<<endl;
    cout<<a<<endl;
    cout<<b;
    return 0;
}

#include <iostream>
using namespace std;

int main() {
    int a[10];
    cout<<"Address of a[0] is "<<&a[0]<<endl;
    cout<<"Address of a[1] is "<<&a[1];
    return 0;
}
