#include <iostream>
using namespace std;
int sample(int a, int b)
{
  int c;
  a=10;
  b=11;
  c=a+b;
  return c;
}
int main()
{
  int a,b;
  a=3;
  b=5;
  cout<<a<<""<<b<<endl;
  int c;
  c=sample(a,b);
  cout<<c<<endl;
  cout<<a<<""<<b<<endl;
  return 0;
}

#include <iostream>
using namespace std;
int sample(int a, int b);
int main()
{
  int a,b;
  a=3;
  b=5;
  cout<<a<<""<<b<<endl;
  int c;
  c=sample(a,b);
  cout<<c<<endl;
  cout<<a<<""<<b<<endl;
  return 0;
}
int sample(int a , int b){
  return a+b;
}

