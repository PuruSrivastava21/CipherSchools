#include <iostream>
#include <string>
using namespace std;
int main() {
    string name;
    cin>>name;
    for(int i=0;i<4;i++){
    cout<<name[i]<<endl;
    }
    return 0;
}

#include <iostream>
#include <string>
using namespace std;
int main() {
    string a;
    string b;
    cin>>a>>b;
    string c;
    c= a+b;
    cout<<c<<endl;
    cout<<c.length();
    return 0;
}

#include <iostream>
#include <string>
using namespace std;
int main() {
    string a;
    string b;
    cin>>a;
    getline(cin,a);
    cout<<a;
    return 0;
}
