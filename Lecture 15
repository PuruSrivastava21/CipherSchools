#include<iostream>
using namespace std;
int compare(int a, int b){
  int c;
  if(a>b){
    c=a;
  }
  else{
    c=b;
  }
return c;
}
OR
#include<iostream>
using namespace std;
int compare(int a, int b){
  if(a>b){
    return a;
  }
  else{
    return b;
  }
}

#include<iostream>
using namespace std;
int compare(int a){
  if(a==5){
    return 1;
  }
  else{
    return 0;
  }
}
